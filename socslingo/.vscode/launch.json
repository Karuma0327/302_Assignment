{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "App",
            "request": "launch",
            "mainClass": "com.socslingo.App",
            "projectName": "socslingo"
        },
        {
            "type": "java",
            "name": "DatabaseController",
            "request": "launch",
            "mainClass": "com.socslingo.controllers.DatabaseController",
            "projectName": "socslingo"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}",
            "vmArgs": "--module-path ${env:PATH_TO_FX} --add-modules javafx.controls,javafx.fxml"
        }
    ]
}