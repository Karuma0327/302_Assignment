{
    "github.copilot.chat.experimental.codeGeneration.instructions": [
        {
            "file": "code-style.md"
        }
    ],
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx16G -Xms1G -Xlog:disable",
    "cSpell.words": [
        "dropshadow",
        "hgrow",
        "javafx",
        "KAHOOT"
    ],
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-21",
            "default": true
        }
    ],
    "cSpell.customDictionaries": {
        "custom-dictionary-workspace": {
            "name": "custom-dictionary-workspace",
            "path": "${workspaceFolder:socslingo}/.cspell/custom-dictionary-workspace.txt",
            "addWords": true,
            "scope": "workspace"
        }
    },
    "maven.view": "flat",
}